@import './vars/colors.styl'
@import './vars/fonts.styl'

body, html
	padding 0
	margin 0
	background-color #fff
	// -webkit-font-smoothing antialiased
	font-size 14px
	font-family default
	
main
	background-color #fff
	padding-top 72px
	margin 0 auto
	
p, span, i, h1, h2, h3, h4
	margin 0

ul
	padding 0
	margin 0

ul li
	list-style none
	
button, a
	outline 0
	
a
	color text
	text-decoration none